{
	"$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
	"name": "sourcetrail",
	"version": "1.0.0",
	"dependencies": [
		"boost-system",
		"boost-date-time",
		"boost-filesystem",
		"boost-program-options",

		"boost-dll",
		"boost-interprocess",
		"boost-uuid",
		"boost-asio",
		"boost-process",
		{
			"name": "boost-locale",
			"features": [
				"icu"
			]
		},

		"sqlite3",

		"tinyxml",

		"icu",

		"qtsvg",
		"qtbase",
		"qtimageformats",
		{
			"name": "llvm",
			"default-features": false,
			"features": [
				"clang",
				"enable-rtti"
			]
		},
		"catch2",
		"gtest"
	]
}
